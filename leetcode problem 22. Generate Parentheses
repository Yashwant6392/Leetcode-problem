class Solution {
public:

    void paranthes(int n,int left,int right,vector<string>&res,string &temp)
    {
        if(left+right==2*n)
        {
            res.push_back(temp);
            return;
        }
        if(left<n)
        {
            temp.push_back('(');
            paranthes(n,left+1,right,res,temp);
            temp.pop_back();
        }
        if(right<left)
        {
            temp.push_back(')');
            paranthes(n,left,right+1,res,temp);
            temp.pop_back();
        }

    }

    vector<string> generateParenthesis(int n) {
        vector<string>res;
        string temp;
        paranthes(n,0,0,res,temp);
        return res;

        
    }
};

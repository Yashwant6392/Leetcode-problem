class Solution {
public:
    int heightChecker(vector<int>& heights) {
        int n=heights.size();
        vector<int>m=heights;
        int count=0;
        sort(m.begin(),m.end());
        int i=0,j=0;
        for(int i=0;i<n;i++)
        {
            for(int j=i;j<=i;j++)
            {
                if(heights[i]!=m[j])
                {
                    count++;
                }

            }
        }
        return count;
        
    }
};
